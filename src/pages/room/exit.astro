---
import BasicHTMLLayout from "layouts/BasicHTMLLayout.astro";
import GoToHome from "components/GoToHome.astro";
---

<BasicHTMLLayout title="Invalid room">
  <div
    class="flex flex-col justify-center items-center h-screen mx-8 text-center"
  >
    <h1 class="text-4xl font-bold mb-4">Removed from Room</h1>
    <p class="mb-4 font-mono">
      Seems like you are no longer allowed in that room
    </p>
    <GoToHome />
  </div>
</BasicHTMLLayout>
